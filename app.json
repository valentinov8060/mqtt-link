{
  "expo": {
    "name": "MQTT Link",
    "slug": "MQTT-Link",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/mqtt-link-icon.png",
    "scheme": "mqttlink",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/mqtt-link-icon.png",
        "backgroundImage": "./assets/images/mqtt-link-icon.png",
        "monochromeImage": "./assets/images/mqtt-link-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.valentinov8047.mqttlink"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/mqtt-link-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4eae8962-a01a-4981-9b4f-d7ff2a956f2d"
      }
    }
  }
}
